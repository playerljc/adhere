{
  "name": "@baifendian/adhere-ui-searchlist",
  "version": "2.10.28",
  "description": "一种查询列表的通用模式(如果 UI 没有明确给出查询列表的 UI，就可以用这个默认模式)",
  "homepage": "https://github.com/playerljc/adhere",
  "bugs": {
    "url": "https://github.com/playerljc/adhere/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/playerljc/adhere"
  },
  "license": "ISC",
  "author": "playerljc",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "lib",
    "es",
    "umd"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startapp:dev": "cross-env mode=development ctbuild startapp --define alias=@,evnVars=true",
    "buildpackage:cjs": "ctbuild buildpackagets-babel -c ctbuild.package.ts.cjs.config.js -o lib",
    "postbuildpackage:cjs": "tsc --declaration -p ./ --emitDeclarationOnly --outDir lib",
    "buildpackage:esm": "ctbuild buildpackagets-babel -c ctbuild.package.ts.esm.config.js -o es",
    "postbuildpackage:esm": "tsc --declaration -p ./ --emitDeclarationOnly --outDir es",
    "buildpackage": "npm-run-all --parallel buildpackage:cjs buildpackage:esm",
    "buildumd": "ctbuild buildumd --config ctbuild.config.umd.js --packagename AdhereSearchTable --define alias=@,evnVars=true",
    "e2e": "cross-env environment=dev mode=development ctbuild startapp -c ../adhere-e2e/ctbuild.e2e.config.js --define alias=@,evnVars=true",
    "generatorchangelog": "bd changelog/CHANGELOG.html"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@baifendian/adhere-ui-conditionalrender": "^2.10.28",
    "@baifendian/adhere-ui-contourblock": "^2.10.28",
    "@baifendian/adhere-ui-css": "^2.10.28",
    "@baifendian/adhere-ui-datedisplay": "^2.10.28",
    "@baifendian/adhere-ui-fieldgeneratortodict": "^2.10.28",
    "@baifendian/adhere-ui-searchtable": "^2.10.28",
    "@baifendian/adhere-util": "^2.10.28",
    "@baifendian/adhere-util-dict": "^2.10.28",
    "@baifendian/adhere-util-intl": "^2.10.28",
    "@ctsj/state": "^2.0.24",
    "antd": "^5.14.0",
    "classnames": "^2.3.2",
    "omit.js": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.23.1",
    "@baifendian/adhere-e2e": "^2.10.28",
    "@ctsj/build": "^6.3.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.11",
    "babel-plugin-import": "^1.13.6",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "antd": ">=5.0.0 - <6.0.0",
    "react": ">=18.0.0 - <19.0.0",
    "react-dom": ">=18.0.0 - <19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
