{
  "name": "@baifendian/adhere-util-domain",
  "version": "2.11.0",
  "description": "domain模块的浏览器端实现",
  "homepage": "https://github.com/playerljc/adhere",
  "bugs": {
    "url": "https://github.com/playerljc/adhere/issues"
