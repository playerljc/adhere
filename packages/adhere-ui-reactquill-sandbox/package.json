  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "umd"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildpackage:cjs": "ctbuild buildpackagets -p tsconfig.cjs.json -o lib",
    "buildpackage:esm": "ctbuild buildpackagets -p tsconfig.esm.json -o es",
    "buildpackage": "npm-run-all --parallel buildpackage:cjs buildpackage:esm",
    "buildumd": "ctbuild buildumd --packagename AdhereaReactquillSandbox --define alias=@,evnVars=true",
    "e2e": "cross-env environment=dev mode=development ctbuild startapp -c e2e/ctbuild.config.js --define alias=@,evnVars=true",
    "generatorchangelog": "bd changelog/CHANGELOG.html"
  },
  "dependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "ahooks": "^3.7.4",
    "classnames": "^2.3.2",
    "omit.js": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.7",
    "@ctsj/build": "^6.0.11",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0 - <19.0.0",
    "react-dom": ">=18.0.0 - <19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
